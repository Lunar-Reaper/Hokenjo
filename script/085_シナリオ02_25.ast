[99]={
name={name="蒼空",ja="蒼空",en="Sora"},
ja={
{
"「そんなことできるのか？」",
},
},
en={
{
"「You can do that?」",
},
},
},
[100]={
vo={
{"vo",file="fem_rin_00393",ch="rin"},
},
name={name="鈴",ja="鈴",en="Rin"},
