[82]={
vo={
{"vo",file="fem_sak_00021",ch="sak"},
},
name={name="咲羽",ja="咲羽",en="Sakiha"},
ja={
{
"「おもしろい妹さんですね」",
},
},
en={
{
"「You have a funny little sister.」",
},
},
},
[83]={
vo={
{"vo",file="fem_shi_00103",ch="shi"},
},
name={name="シロバナ",ja="シロバナ",en="Shirobana"},
