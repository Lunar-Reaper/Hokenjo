[72]={
name={name="蒼空",ja="蒼空",en="Sora"},
ja={
{
"「あ、えっと」",
},
},
en={
{
"「Ah, well.」",
},
},
},
[73]={
ja={
{
"どう答えようか迷う。",
},
},
en={
{
"I wasn't sure how to answer.",
},
},
