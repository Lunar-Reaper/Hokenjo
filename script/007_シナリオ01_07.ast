[51]={
vo={
{"vo",file="fem_shi_00098",ch="shi"},
},
name={name="シロバナ",ja="シロバナ",en="Shirobana"},
ja={
{
"「実に無能ね」",
},
},
en={
{
"「Truly incompetent.」",
},
},
},
[52]={
name={name="蒼空",ja="蒼空",en="Sora"},
ja={
{
"「すみません、人数をふたりに変更できますか？　部屋はひとつで問題ないので」",
},
},
en={
{
"「I'm sorry, but can you change it to two people? We only need one room.」",
},
