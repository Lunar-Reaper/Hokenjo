[268]={
vo={
{"vo",file="fem_z03_00099",ch="shi"},
},
name={name="シロバナ",ja="白花",en="Shiroka"},
ja={
{
"「…………」",
},
},
en={
{
"「......」",
},
},
},
[269]={
ja={
{
"そのとき、白花と目が合った。",
