[256]={
name={name="蒼空",ja="蒼空",en="Sora"},
ja={
{
"「なら俺の仕事を手伝ってくれ」",
},
},
en={
{
"「Then help me out here.」",
},
},
},
[257]={
vo={
{"vo",file="fem_shi_00265",ch="shi"},
